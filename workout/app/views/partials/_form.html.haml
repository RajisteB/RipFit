= simple_form_for(@workout, url: create_workout_path, html: {class: 'form-horizontal'}) do |f|
    = f.input :name, label: "Name", input_html: {class: "form-control"}
    = f.input :category, label: "Category", input_html: {class: "form-control"}
    = f.input :reps, label: "# of Reps", input_html: {class: "form-control"}
    = f.input :progression, label: "Progression Level", input_html: {class: "form-control"}
    = f.input :example, label: "YouTube Embed Link", input_html: {class: "form-control"}
    %br 
    = f.button :submit